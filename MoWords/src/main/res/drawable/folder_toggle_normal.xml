<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <gradient
                android:startColor="#ffece3"
                android:endColor="#c58b1c"
                android:angle="270" />
            <stroke
                android:width="0dp"
                android:color="#c58b1c"/>
            <corners
                android:radius="16dp" />
            <padding
                android:left="10dp"
                android:top="4dp"
                android:right="10dp"
                android:bottom="4dp" />
        </shape>
    </item>
</layer-list>
